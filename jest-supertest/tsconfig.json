{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./automation",
    "alwaysStrict": true,
  },
  "quotePreference": "double",
  "include": ["automation/**/*"],
  "exclude": ["node_modules", "dist"]
}
